generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type GlobalesCartasCodigoMateria {
  CASEI String
  CETYS String
}


model Cartas {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model globales_Cartas {
  id                           String                                   @id @default(auto()) @map("_id") @db.ObjectId
  CODIGO_MATERIA               GlobalesCartasCodigoMateria
}
