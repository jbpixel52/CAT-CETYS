model Cartas {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model globales_Cartas {
  id                                     String                                      @id @default(auto()) @map("_id") @db.ObjectId
  //version                                Int                                         @unique @db.Int 
  NOMBRE_MATERIA                         GlobalesCartasNombreMateria                 //CASEI 2   | ABET 1  | CETYS GENERALES
  REQUISITOS_MATERIA                     GlobalesCartasRequisitos                    //CASEI 3 
  NOMBRE_CARRERA                         GlobalesCartasNombreCarrera                 // 
  CLAVE_MATERIA                          GlobalesCartasClaveMateria                  //CASEI 1   | ABET 1
  SEMESTRE                               GlobalesCartasSemestre                      //CASEI 4 
  NIVEL_ACADEMICO                        GlobalesCartasNivelAcademico                //          |         | CETYS GENERALES
  TIPO_CURSO                             GlobalesCartasTipoMateria                   //CASEI 5 
  EJES                                   GlobalesEjes                                //CASEi 6-
  MODALIDAD                              GlobalesModalidad                           //          |         | CETYS GENERALES
  INSTALACIONES                          GlobalesInstalaciones                       //                    | CETYS GENERALES
  IDIOMA                                 GlobalesCartasIdioma                        //                    | CETYS GENERALES
  CREDITOS                               GlobalesCreditos                            //          | ABET 2  | CETYS GENERALES
  HORAS_CLASE                            GlobalesHorasClase                          //CASEI 6   |         | CETYS GENERALES
  HORAS_CLASE_SEMANALES                  GlobalesHorasClaseSemanales                 //CASEI 10a |         | CETYS GENERALES
  HORAS_CLASE_INDEPENDIENTES             GlobalesHorasClaseIndependientes            //          |         | CETYS GENERALES
  HORAS_CLASE_INDEPENDIENTES_SEMANALES   GlobalesHorasClaseIndependientesSemanales   //          |         | CETYS GENERALES
  HORAS_CLASE_TOTALES                    GlobalesHorasClaseTotales                   //          |         | CETYS GENERALES
  HORAS_CLASE_TOTALES_SEMANALES          GlobalesHorasClaseTotalesSemanales          //          |         | CETYS GENERALES
  OBJETIVOS_CURSO                        GlobalesObjetivosCurso                      //CASEI 7   | ABET 6  | CETYS PROPOSITO Y COMPETENCIA
  COMPETENCIAS                           GlobalesCompetencias                        //          |         | CETYS UNIDADES Y COMPETENCIA
  METODOS_ACTIVIDADES                    GlobalesMetodosActividades                  //          |         | CETYS Metodos y actividades
  ACTIVIDADES_INDEPENDIENTES             GlobalesActividadesIndependientes           //          |         | CETYS Metodos y actividades
  METODOLOGIA_EVALUACION                 GlobalesMetodologiaEvaluacion               //          |         | CETYS Evaluacion
  INSTRUMENTOS_MEDICION_APRENDIZAJE      GlobalesInstrumentosMedicion                //          |         | CETSY Evaluacion
  APORTACION_CURSO_PE                    GlobalesAportacionCursoPE                   //CASEI 8 
  APORTACION_CURSO_CACEI                 GlobalesAportacionCursoCACEI                //CASEI 9 
  INFORMACION_ESPECIFICA                 GlobalesInformacionEspecifica
  EVIDENCIAS                             GlobalesEvidencias                          //          |         | CETYS COMPETENCIA
  DATOS_RELEVANTES_CURSO                 GlobalesDatosRelevantesCurso                //CASEI 10 
  UNIDAD                                 GlobalesUnidad
  TEMAS_CURSO                            GlobalesTemasCurso                          //CASEI 11  | ABET 7  | CETYS UNIDADES
  ESTRATEGIAS_ENSENANZA                  GlobalesEstrategiasEnsenanza                //CASEI 12 
  ESTRATEGIAS_EVALUACION                 GlobalesEstrategiasEvaluacion               //CASEI 13 
  PRACTICAS                              GlobalesPracticas                           //CASEI 14 
  RETROALIMENTACION                      GlobalesRetroalimentacion                   //          |         | CETYS RETROALIMENTACION
  CRITERIOS_EVALUACION                   GlobalesCriteriosEvaluacion                 //          |         | CETYS EVALUACION
  BIBLIOGRAFIA                           GlobalesBibliografia                        //CASEI 15  | ABET 4
  PROFESORADO                            GlobalesProfesorado                         //CASEI 16  |         | CETYS PROFESOR     
}

model Campo{
  id                                     String                                      @id @default(auto()) @map("_id") @db.ObjectId
  NOMBRE_CAMPO                           String
}